:0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【海坂】"]},{"code":401,"indent":0,"parameters":["「曾經的人……都處在戰爭之中。"]},{"code":401,"indent":0,"parameters":["　這種事，在我們的世界裡是無法想像的」"]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":4,"y":5},{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":143},"list":[{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["【資料B】"]},{"code":401,"indent":0,"parameters":["「逃脫者的記錄。"]},{"code":401,"indent":0,"parameters":["　『關於天使』」"]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["【資料B】"]},{"code":401,"indent":0,"parameters":["「最早在古巴比倫尼亞帝國的資料上出現過記述，"]},{"code":401,"indent":0,"parameters":["　從公元前到２１世紀為止都有所活動的超常現象。"]},{"code":401,"indent":0,"parameters":["　詳細不明（後述）」"]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["【資料B】"]},{"code":401,"indent":0,"parameters":["「由解析各個時代根源意識的機構"]},{"code":401,"indent":0,"parameters":["　prajna嘗試進行了研究，"]},{"code":401,"indent":0,"parameters":["　但除了天使的表層作用以外都無法解析」"]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["【資料B】"]},{"code":401,"indent":0,"parameters":["「這表示天使的起源存在於基層現實以外。也有研究者"]},{"code":401,"indent":0,"parameters":["　將其當做天使處於上位世界的證據，但真相不得而知」"]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["【資料B】"]},{"code":401,"indent":0,"parameters":["「據稱天使有『實現不幸之人的願望』的能力。"]},{"cod